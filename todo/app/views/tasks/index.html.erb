<h1>Tasks</h1>
<%= link_to 'new', new_task_path  %>
<ul>
  <% @tasks.each do |task| %>
    <li>
      <%= check_box_tag '', '' %>
      <%= task.title %>
      <%= link_to '[編集]',edit_task_path(task) %>
      <%= link_to '[削除]', task_path(task), data: {turbo_method: 'delete', turbo_confirm: '消していいの？'} %>
    </li>
  <% end %>
</ul>
